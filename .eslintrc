{
  "env": {
    "browser": true,
    "es2021": true,
    "node": true
  },
  "extends": ["eslint:recommended", "prettier", "airbnb"],
  "parserOptions": {
    "ecmaFeatures": {
      "jsx": true
    },
    "ecmaVersion": 12,
    "sourceType": "module"
  },
  "plugins": ["prettier"],
  "rules": {
    "react/prop-types": "off",
    "prettier/prettier": "error",
    "linebreak-style": ["error", "unix"],
    "comma-dangle": ["error", "only-multiline"]
  }
}
